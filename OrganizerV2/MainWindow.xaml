<Window x:Class="OrganizerV2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:viewmodels="clr-namespace:OrganizerV2.ViewModels" xmlns:views="clr-namespace:OrganizerV2.Views"
        mc:Ignorable="d"
        Height="auto"
        MinWidth="320"
        ResizeMode="CanMinimize"
        SizeToContent="WidthAndHeight"
        Topmost="True"
        Title="Dupa123"
        WindowStyle="ToolWindow">

    <Window.DataContext>
        <viewmodels:MainViewModel/>
    </Window.DataContext>

    <StackPanel Margin="3">
        <views:ViewPropertiesView/>
        <views:SelectedObjectView/>
    </StackPanel>
</Window>
